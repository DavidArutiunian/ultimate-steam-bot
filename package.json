{
  "name": "ultimate-steam-bot",
  "version": "0.0.1",
  "description": "Ultimate Steam Bot",
  "main": "index.js",
  "author": "David Arutiunian",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.15",
    "yarn": ">=1.12"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.5",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^6.1.0",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-bunyan-logger": "^1.3.0",
    "@types/helmet": "^0.0.42",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/redis": "^2.8.10",
    "@types/steam-totp": "^2.0.0",
    "@types/steamid": "^1.1.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "compression": "^1.7.3",
    "core-decorators": "^0.20.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-bunyan-logger": "^1.3.3",
    "globaloffensive": "^1.3.2",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "module-alias": "^2.1.0",
    "redis": "^2.8.0",
    "steam-totp": "^2.1.0",
    "steam-tradeoffer-manager": "^2.9.4",
    "steam-user": "^3.29.1",
    "steamcommunity": "^3.38.0",
    "steamid": "^1.1.0",
    "typescript": "^3.2.2",
    "utility-types": "^3.2.0"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "start": "yarn run serve",
    "build": "yarn run clean && tsc",
    "serve": "node -r dotenv/config -r module-alias/register dist/index.js | bunyan",
    "watch-node": "cross-env NODE_ENV=development nodemon -r module-alias/register dist/index.js | bunyan",
    "watch-ts": "tsc -w",
    "tslint": "tslint -p tsconfig.json",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": " cross-env NODE_ENV=test mocha --opts mocha.opts",
    "coverage": "yarn run clean && cross-env NODE_ENV=test nyc mocha --opts mocha.opts",
    "watch-test": "cross-env NODE_ENV=test mocha --opts mocha.opts --watch-extensions ts --watch"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.8.0"
  },
  "_moduleAliases": {
    "@utils": "dist/utils",
    "@entities": "dist/entities",
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@routes": "dist/routes",
    "@app": "dist/app",
    "@config": "dist/config",
    "@middlewares": "dist/middlewares"
  }
}
